                  Abstract.obj      \
                  ClassObject.obj   \
                  CObject.obj       \
                  ComplexObject.obj \
                  DictObject.obj    \
                  FileObject.obj    \
                  FloatObject.obj   \
                  FrameObject.obj   \
                  FuncObject.obj    \
                  ListObject.obj    \
                  LongObject.obj    \
                  MethodObject.obj  \
                  ModuleObject.obj  \
                  Object.obj        \
                  RangeObject.obj   \
                  SliceObject.obj   \
                  StringObject.obj  \
                  TupleObject.obj   \
