    <RULE ID="VID_EspressoDrinks" TOPLEVEL="ACTIVE">
        <O>
            <L>
                <P>May I have</P>
                <P>Can I have</P>
                <P>Can I get</P>
                <P>Please get me</P>
                <P>Get me</P>
                <P>I'd like</P>
                <P>I would like</P>
            </L>
        </O>
        <O>a</O>
		<P MIN="1" MAX="7">
		    <RULEREF REFID="VID_OrderList"/>
		</P>
        <O>please</O>
    </RULE>
    <RULE ID="VID_Navigation" TOPLEVEL="ACTIVE">
        <O>Please</O>
        <P>
            <L>
                <P>Enter</P>
                <P>Go to</P>
            </L>
        </P>
        <O>the</O>
        <RULEREF REFID="VID_Place" /> 
    </RULE>
    <RULE ID="VID_Place" >
