            <RULEREF REFID="VID_IcedOrNot" PROPID="VID_IcedOrNot"/> 
            <RULEREF REFID="VID_DecafOrNot" PROPID="VID_DecafOrNot"/> 
            <RULEREF REFID="VID_Shots" PROPID="VID_Shots"/> 
