// Choreo version 1
event section "wait till looking at player"
{
  time 0.450000 -1.000000
  param "noaction"
}
event stoppoint "done"
{
  time 4.660000 -1.000000
  param "noaction"
}
actor "lodge_citizen"
{
  channel "audio"
  {
    event speak "ep_02.cit_tothesawmill_03"
    {
      time 0.500000 3.700000
      param "ep_02.cit_tothesawmill_03"
      fixedlength
      cctype "cc_master"
      cctoken ""
    }
  }
  channel "move to"
  {
    event moveto "mark_lodge_citizen_move"
    {
      time 1.953333 3.006666
      param "mark_lodge_citizen_move"
      param2 "Walk"
      forceshortmovement
    }
  }
  channel "face to"
  {
    event face "face player"
    {
      time 0.006667 2.260000
      param "!player"
      event_ramp
      {
        0.7353 1.0000
        1.8573 1.0000
      }
      resumecondition
    }
    event face "mark_lodge_citizen_sawmill"
    {
      time 1.986666 4.386667
      param "mark_lodge_citizen_sawmill"
      event_ramp
      {
        0.4813 0.3022
        0.9639 0.4067
      }
    }
    event face "face player"
    {
      time 3.026667 5.586667
      param "!player"
      event_ramp
      {
        1.0994 0.8103 "curve_catmullrom_normalize_x_to_curve_catmullrom_normalize_x"
        1.6460 1.0000 "curve_easein_to_curve_easeout"
        2.0540 1.0000
        2.5600 0.0000
      }
    }
  }
  channel "lookAt"
  {
    event lookat "!player"
    {
      time 0.039999 5.579999
      param "!player"
      event_ramp
      {
        0.4760 1.0000
        4.9480 1.0000
        5.5400 0.0000
      }
    }
  }
  channel "gesture"
  {
    event gesture "g_pointleft_l"
    {
      time 1.966667 4.433333
      param "g_pointleft_l"
      absolutetags playback_time
      {
        "in" 0.068493
        "loop" 0.397260
        "end" 0.534247
        "out" 0.712329
      }
      absolutetags shifted_time
      {
        "in" 0.068493
        "loop" 0.397260
        "end" 0.534247
        "out" 0.712329
      }
      sequenceduration 2.466667
    }
  }
}

scalesettings
{
  "CChoreoView" "100"
  "RampTool" "100"
  "ExpressionTool" "100"
  "GestureTool" "100"
  "SceneRampTool" "100"
}
fps 60
snap off
