// Choreo version 1
event section "wait till alyx reaches the gate"
{
  time 0.884616 -1.000000
  param "noaction"
}
actor "Alyx"
{
  channel "move to"
  {
    event moveto "move to dog by gate"
    {
      time -0.051282 3.948718
      param "alyx_dogatgate_moveto"
      param2 "Run"
      resumecondition
    }
  }
  channel "face to"
  {
    event face "face player"
    {
      time 3.000000 5.000000
      param "!player"
      event_ramp
      {
        0.4660 0.9892
      }
    }
    event face "face dog"
    {
      time 1.000000 3.000000
      param "dog"
      event_ramp
      {
        0.4300 0.9783
      }
    }
  }
  channel "triggers"
  {
  }
  channel "audio"
  {
    event speak "gooddoggie"
    {
      time 0.987180 2.672531
      param "ep_02.al_str_gooddoggie"
      fixedlength
      cctype "cc_master"
      cctoken ""
    }
  }
  channel "lookat"
  {
    event lookat "look at dog"
    {
      time 1.000000 3.000000
      param "dog"
    }
    event lookat "look at player"
    {
      time 3.000000 5.000000
      param "!player"
    }
  }
}

scalesettings
{
  "CChoreoView" "52"
  "RampTool" "100"
  "ExpressionTool" "100"
  "GestureTool" "100"
  "SceneRampTool" "100"
}
fps 60
snap off
